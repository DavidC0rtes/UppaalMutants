		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-127" y="-85">appr!</label>
		</transition>
	</template>
	<template>
		<name>Template0</name>
		<location id="id3" x="-136" y="-34">
			<name x="-195" y="-42">zero</name>
		</location>
		<location id="id4" x="-34" y="-34">
			<name x="-44" y="-68">one</name>
		</location>
		<location id="id5" x="68" y="-34">
			<name x="58" y="-68">two</name>
		</location>
		<location id="id6" x="-34" y="59">
			<name x="-42" y="76">three</name>
		</location>
		<location id="id7" x="-42" y="-127">
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id7"/>
			<target ref="id5"/>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id7"/>
			<label kind="synchronisation" x="-118" y="-97">appr?</label>
		</transition>
		<transition>
			<source ref="id6"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-127" y="17">raise!</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="25" y="8">leave?</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="-16" y="-51">lower!</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id4"/>
			<label kind="synchronisation" x="-118" y="-51">appr?</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
Process1 = Template0();
// List one or more processes to be composed into a system.
system Process,Process1;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
