			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-76" y="-178">leave!</label>
			<nail x="-59" y="-153"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-41" y="-102">enter!</label>
