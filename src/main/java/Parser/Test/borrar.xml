<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
chan leave, enter, appr, raise, lower;</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-476" y="-153">
		</location>
		<location id="id1" x="-391" y="-153">
		</location>
		<location id="id2" x="-289" y="-153">
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id2"/>
			<target ref="id0"/>
			<label kind="synchronisation" x="-416" y="-255">leave!</label>
			<nail x="-391" y="-229"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="-373" y="-170">enter!</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="-458" y="-170">appr!</label>
		</transition>
	</template>
	<template>
		<name>Template0</name>
		<location id="id3" x="-280" y="-68">
		</location>
		<location id="id4" x="-178" y="-68">
		</location>
		<location id="id5" x="-68" y="-68">
		</location>
		<location id="id6" x="-178" y="42">
		</location>
		<init ref="id3"/>
		<transition>
			<source ref="id6"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-272" y="-8">raise!</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id6"/>
			<label kind="synchronisation" x="-110" y="-8">leave?</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="-153" y="-85">lower!</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id4"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
Process2 = Template0();
// List one or more processes to be composed into a system.
system Process, Process2;</system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
