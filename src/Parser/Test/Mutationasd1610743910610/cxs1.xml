<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
<declaration>chan a ;
clock x ;
</declaration>
<template>
<name x="5" y="5">Template</name>
<declaration></declaration>
<location id="id0" x="0" y="0">
<name x="-10" y="-34">inicial</name>
<label kind="invariant" x="-8" y="17">x&lt;=10</label>
</location>
<location id="id1" x="187" y="0">
</location>
<init ref="id0"/>
<transition>
<source ref="id0"/>
<target ref="id1"/>
<label kind="guard" x="76" y="-25">x &gt;= (10-1) - 1</label>
</transition>
</template>
<template>
<name>Template0</name>
<location id="id2" x="0" y="0">
</location>
<location id="id3" x="119" y="0">
</location>
<init ref="id2"/>
<transition>
<source ref="id2"/>
<target ref="id3"/>
<label kind="synchronisation" x="18" y="-17">a?</label>
<label kind="guard" x="18" y="-34">x &gt;= 10</label>
</transition>
</template>
<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Template, Template0;
    </system>
<queries>
<query>
<formula></formula>
<comment></comment>
</query>
</queries>
</nta>